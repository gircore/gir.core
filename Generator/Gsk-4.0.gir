              c:identifier="GSK_TRANSFORM_CATEGORY_ANY"
              glib:nick="any">
        <doc xml:space="preserve"
             filename="../gsk/gskenums.h"
             line="178">Analyzing the matrix concluded that it does
    not fit in any other category.</doc>
      </member>
      <member name="3d"
              value="2"
